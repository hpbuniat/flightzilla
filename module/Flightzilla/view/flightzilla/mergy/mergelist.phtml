<?php
if (empty($this->aMergyStack) === true) {
    $this->aMergyStack = array();
}

foreach ($this->aMergyStack as $this->oStack) {

    /* @var $this->oStack \Flightzilla\Model\Mergy\Revision\Stack */
    if ($this->oStack->isEmpty() !== true) {
        $this->aRevisions = $this->oStack->getRaw();
        echo $this->render('flightzilla/mergy/partial/revisions.phtml');
    }
}
