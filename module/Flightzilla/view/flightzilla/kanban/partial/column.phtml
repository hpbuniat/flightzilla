<?php
$this->iRendered = 0;
foreach ($this->aStack as $this->oTicket) {
    if ($this->oTicket instanceof \Flightzilla\Model\Ticket\AbstractType) {
        if ($this->oTicket->isChangedWithinLimit($this->oConfig->tickets->workflow->board) === true) {
            $this->iRendered++;
            echo $this->render('flightzilla/kanban/partial/pin.phtml');
        }
    }
}
